<!DOCTYPE html>
<html lang="en">
<%- include('head') %> 
<body>
    <%- include('navigation') %> 

    <div>
        <% for( let i=0; i < newArray.length; i++ ) { %>
            <% if (newArray[i].volumeInfo.imageLinks) { %>
                <img src=<%=newArray[i].volumeInfo.imageLinks.thumbnail %> alt="">
                <% } %>
                    <h3>
                        <%= newArray[i].volumeInfo.title %>
                    </h3>
                    <h5>
                        <%= newArray[i].volumeInfo.authors %>
                    </h5>
                    <div>
                        <span>
                            <%= newArray[i].volumeInfo.pageCount %>
                                <%= newArray[i].volumeInfo.categories %>
                                    <%= newArray[i].id %>
                        </span>
                    </div>
                    <% } %>

    </div>


    <script type="module" src="https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.esm.js"></script>
            <script nomodule src="https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.js"></script>

            <script src="bundle.js"></script>
    
</body>
</html>